    "def data_to_dictionary(data):\n",
    "    '''Takes in data and makes a dictionary where the keys are the destinations and the\n",
    "    values are all the connected airports to that destination airport'''\n",
    "    D = {}\n",
    "    for i in data:\n",
    "        if(i[0] not in D):\n",
    "            D[i[0]] = [i[1]]\n",
    "        else:\n",
    "            D[i[0]].append(i[1])\n",
    "            D[i[0]].sort()\n",
    "    \n",
    "    return D\n",
    "\n",
    "data_to_dictionary(data)"
