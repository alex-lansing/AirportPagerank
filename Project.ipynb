    "import random\n",
    "\n",
    "class PR_Iterator:\n",
    "    \n",
    "    def __init__(self, graph):\n",
    "        '''Takes in graph, creates instance variable that stores PR_DiGraph object\n",
    "        initializes i as 0 to count steps taken, sets initial value to random airport'''\n",
    "        self.graph = graph\n",
    "        self.i = 0\n",
    "        self.current_state = \"KZN\"\n",
    "        \n",
    "    def follow_link(self):\n",
    "        '''Randomly chooses new airport served by current airport,\n",
    "        jump from one airport to another, teleport if jump to same airport'''\n",
    "        \n",
    "        #randomly choose next airport\n",
    "        try:\n",
    "            next_state = random.choice(list(self.graph.link_dict[self.current_state]))\n",
    "            \n",
    "            #check if newly chosed airport matches the current airport; update if different, teleport if same\n",
    "            if next_state != self.current_state:\n",
    "                self.current_state = next_state\n",
    "            else:\n",
    "                self.teleport()         \n",
    "        #Key Error handling\n",
    "        except KeyError:\n",
    "            self.teleport()\n",
    "        \n",
    "        \n",
    "    def teleport(self):\n",
    "        '''Set new state by changing current location to new randomly chosen location'''\n",
    "        self.current_state = random.choice(list(self.graph.link_dict.keys()))\n",
    "        \n",
    "    def __next__(self):\n",
    "        '''Execute follow_link with 85% probability, execute teleport with 15% probability\n",
    "        Ensure iteration limit is not exceeded, stop if limit is met, return current state'''\n",
    "        \n",
    "        #check if current amount of steps has reached limit, stop if reached\n",
    "        if self.i == self.graph.iteration_limit:\n",
    "            raise StopIteration\n",
    "        \n",
    "        #add to steps if limit not exceeded\n",
    "        self.i+=1\n",
    "        \n",
    "        #85% chance execute follow_link, 15% chance execute teleport\n",
    "        if random.random() < 0.85:\n",
    "            self.follow_link()\n",
    "        else:\n",
    "            self.teleport()\n",
    "            \n",
    "        return self.current_state"
   ]
